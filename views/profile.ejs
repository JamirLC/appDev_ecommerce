<%- include('partials/navbar') %>

    <!-- Start Profile Card -->
    <div class="cont">
        <div class="container">
            <div class="card shadow-sm">
                <div class="card-body">
                    <h4 class="card-title mb-4">Personal Information</h4>
                    <div class="row">
                        <div class="col-md-6">
                            <div class="info-item mb-3">
                                <i class="bi bi-person-fill fs-4  me-2"></i>
                                <div class="info-text">
                                    <strong>Last Name, First</strong><br> 
                                    <small class="text-muted">Name</small>
                                </div>
                            </div>
                            <div class="info-item mb-3">
                                <i class="bi bi-envelope-fill fs-4 me-2"></i> 
                                <div class="info-text">
                                    <strong>example@gmail.com</strong><br> 
                                    <small class="text-muted">Email</small>
                                </div>
                            </div>
                        </div>
                        <div class="col-md-6">
                            <div class="info-item mb-3">
                                <i class="bi bi-heart-fill fs-4 me-2"></i>
                                <div class="info-text">
                                    <strong>Single</strong><br>
                                    <small class="text-muted">Marital Status</small>
                                </div>
                            </div>
                            <div class="info-item mb-3">
                                <i class="bi bi-flag-fill fs-4 me-2"></i>
                                <div class="info-text">
                                    <strong>Filipino</strong><br>
                                    <small class="text-muted">Nationality</small>
                                </div>
                            </div>
                        </div>
                    </div>
                </div>
            </div>
        </div>
    </div>
    <!-- End Profile Card -->

    <!-- Bootstrap JS -->
    <script src="https://cdn.jsdelivr.net/npm/bootstrap@5.3.0-alpha1/dist/js/bootstrap.bundle.min.js"></script>
</body>
</html>
